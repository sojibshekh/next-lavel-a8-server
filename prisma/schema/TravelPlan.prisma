model TravelPlan {
  id          String     @id @default(cuid())
  user        User       @relation(fields: [userId], references: [id])
  userId      String
  photoUrl    String?
  destination String
  startDate   DateTime
  endDate     DateTime
  budget      Float
  travelType  TravelType
  description String?
  reviews     Review[]
  matches     Match[]
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @default(now()) @updatedAt
}
