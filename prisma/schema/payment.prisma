model Payment {
  id              String   @id @default(uuid())
  userId          String
  subscriptionId  String
  transactionId   String   @unique
  amount          Float
  status          String
  invoiceUrl      String?
  paymentGatewayData Json?
  createdAt       DateTime @default(now())

  user User @relation(fields: [userId], references: [id])
}
